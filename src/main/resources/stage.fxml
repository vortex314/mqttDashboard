<AnchorPane maxHeight="600" minHeight="350.0" prefHeight="350.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <Button fx:id="bookButton" defaultButton="true" layoutX="203.0" layoutY="300.0" mnemonicParsing="false" onAction="#sayHello" text="Book room" textAlignment="CENTER" textFill="#2343e4">
