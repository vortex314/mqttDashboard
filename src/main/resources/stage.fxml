    <ColorPicker layoutX="386.0" layoutY="74.0" />
    <Slider fx:id="slider" layoutX="103.0" layoutY="104.0" />
    <LineChart fx:id="linechart" layoutX="281.0" layoutY="120.0" prefHeight="200" prefWidth="300">
